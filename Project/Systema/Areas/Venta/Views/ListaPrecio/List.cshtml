<h1>LISTA DE PRECIOS</h1>
<div class="row">
    <div class="col-md-12">
        <div class="portlet box blue-dark ">

            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-gift"></i>Búsqueda
                </div>
                <div class="tools">
                    <a href="" class="collapse">
                    </a>

                    <a href="" class="reload">
                    </a>

                </div>
            </div>




            <div class="portlet-body" id="blockui_List_portlet_body">
                <tabset>
                    <tab heading="Proveedores">
                        <div class="table-responsive">


                            <div class="col-md-13 portlet-body">
                                <form role="form" class="form-inline" id="myform" name="myform">
                                    <div class="form-group">
                                        <label class="control-label">Descripcion</label>
                                        <input type="text" class="form-control" placeholder="" ng-click="List()"
                                               ng-model="parameters.Descripcion_Producto">

                                        <label class="control-label">Categoria</label>
                                        <select id="MySelect" ng-model="parameters.Nombre" class="form-control" ng-click="List()"
                                                data-ng-options="a.Nombre as a.Nombre for a in CategoriaList"></select>

                                        <label class="control-label">PROVEEDOR</label>
                                        <select id="SELECT2" ng-model="parameters.Nombres_Socio" class="form-control" ng-click="List()"
                                                data-ng-options="b.Nombres_Socio as b.Nombres_Socio for b in SocioNegocioList"></select>



                                    </div>

                                    <div class="form-group">
                                        <button type="button" class="btn blue-dark" ng-click="List()">BUSCAR</button>
                                    </div>







                                </form>
                            </div>



                            <div>
                                <div class="row">

                                </div>

                            </div>


                            <table class="table table-striped table-bordered table-hover">
                                <thead>
                                    <tr>

                                        <th>
                                            ID
                                        </th>
                                        <th>
                                            CODIGO
                                        </th>
                                        <th>
                                            DESCRIPCION
                                        </th>
                                        <th>
                                            MARCA
                                        </th>
                                        <th>
                                            CATEGORIA
                                        </th>
                                        <th>
                                            SUBCATEGORIA
                                        </th>
                                        <th>
                                            ESTADO
                                        </th>

                                        <th>
                                            PRECIO COMPRA
                                        </th>


                                        <th></th>

                                    </tr>
                                </thead>
                                <tbody>

                                    <tr ng-repeat="item in ALTProductoBySocioList">
                                        <td>{{item.ID_Producto}}</td>
                                        <td>{{item.Codigo_Producto}}</td>
                                        <td>{{item.Descripcion_Producto}}</td>
                                        <td>{{item.ID_Marca}}</td>
                                        <td>{{item.ID_Categoria}}</td>
                                        <td>{{item.ID_SubCategoria}}</td>
                                        <td>{{item.NombreEstado}}</td>
                                        <td>{{item.Precios}}</td>


                                    </tr>
                                </tbody>
                            </table>

                        </div>
                    </tab>

                    <tab heading="Clientes" >
                        <div class="table-responsive">


                            <div class="col-md-13 portlet-body">
                                <form role="form" class="form-inline" id="myform" name="myform">
                                    <div class="form-group">
                                        <label class="control-label">Descripcion</label>
                                        <input type="text" class="form-control" placeholder="" ng-click="XList()"
                                               ng-model="Xparameters.Descripcion_Producto">

                                        <label class="control-label">Categoria</label>
                                        <select id="MySelect" ng-model="Xparameters.Nombre" class="form-control" ng-click="XList()"
                                                data-ng-options="a.Nombre as a.Nombre for a in CategoriaList"></select>

                                        <label class="control-label">CLIENTE</label>
                                        <select id="SELECT2" ng-model="Xparameters.Nombres_Socio" class="form-control" ng-click="XList()"
                                                data-ng-options="b.Nombres_Socio as b.Nombres_Socio for b in CSocioList"></select>



                                    </div>

                                    <div class="form-group">
                                        <button type="button" class="btn blue-dark" ng-click="XList()">BUSCAR</button>
                                    </div>







                                </form>
                            </div>



                            <div>
                                <div class="row">

                                </div>

                            </div>


                            <table class="table table-striped table-bordered table-hover">
                                <thead>
                                    <tr>

                                        <th>
                                            ID
                                        </th>
                                        <th>
                                            CODIGO
                                        </th>
                                        <th>
                                            DESCRIPCION
                                        </th>
                                        <th>
                                            MARCA
                                        </th>
                                        <th>
                                            CATEGORIA
                                        </th>
                                        <th>
                                            SUBCATEGORIA
                                        </th>
                                        <th>
                                            ESTADO
                                        </th>

                                        <th>
                                            PRECIO VENTA
                                        </th>


                                        <th></th>

                                    </tr>
                                </thead>
                                <tbody>

                                    <tr ng-repeat="item in XALTProductoBySocioList">
                                        <td>{{item.ID_Producto}}</td>
                                        <td>{{item.Codigo_Producto}}</td>
                                        <td>{{item.Descripcion_Producto}}</td>
                                        <td>{{item.ID_Marca}}</td>
                                        <td>{{item.ID_Categoria}}</td>
                                        <td>{{item.ID_SubCategoria}}</td>
                                        <td>{{item.NombreEstado}}</td>
                                        <td>{{item.Precios}}</td>


                                    </tr>
                                </tbody>
                            </table>

                        </div>
                    </tab>

                </tabset>
            </div>

        </div>



    </div>
</div>


<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>

<script>

    function limpiarSelect(id) {
        var select = document.getElementById(id);
        while (select.length > 0) {
            select.remove("%");
        }
    }

</script>

